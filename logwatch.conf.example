misc={

#开启/关闭应用
	enable=yes

#配置系统log路径，指定一个默认为可读写的分区,不确定则使用/data,如果指定分区不可写，软件将
#在/data/分区默认建立ingenic-log目录存放log
	log_path=/data

#配置保存log的数量
	log_num=3
}

kmsg={

#是否记录内核log:yes or no
	is_enable=yes

#参数目前不支持
	fifo_size=0

#参数目前不支持
	prior=6
}

logcat={

#是否开启logcat:yes or no
	is_enable=yes

#参数目前不支持
	fifo_size=0

#logcat输出等级
#  V(6)    Verbose
#  D(5)    Debug
#  I(4)    Info
#  W(3)    Warn
#  E(2)    Error
#  F(1)    Fatal
#  S(0)    Silent (supress all output)
	prior=5
}
